@model InstruktorerPassMVC.Models.Instruktorer

@{
    ViewData["Title"] = "Delete Instruktor";
}

<h2>Är du säker på att du vill radera vald instruktör?</h2><br/>
<h4>Instruktör: @Html.DisplayFor(model => model.In_Id)</h4>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Id
        </dt>
        <dd>
            @Html.DisplayFor(model => model.In_Id)
        </dd>
        <dt>
            Förnamn
        </dt>
        <dd>
            @Html.DisplayFor(model => model.In_Fornamn)
        </dd>
        <dt>
            Efternamn
        </dt>
        <dd>
            @Html.DisplayFor(model => model.In_Efternamn)
        </dd>
        <dt>
            Adress
        </dt>
        <dd>
            @Html.DisplayFor(model => model.In_Adress)
        </dd>
        <dt>
            Postnummer
        </dt>
        <dd>
            @Html.DisplayFor(model => model.In_Postnummer)
        </dd>
        <dt>
            Ort
        </dt>
        <dd>
            @Html.DisplayFor(model => model.In_Ort)
        </dd>
        <dt>
            Epost
        </dt>
        <dd>
            @Html.DisplayFor(model => model.In_Epost)
        </dd>
        <dt>
            Mobilnummer
        </dt>
        <dd>
            @Html.DisplayFor(model => model.In_Mobil)
        </dd>
        <dt>
            Personnummer
        </dt>
        <dd>
            @Html.DisplayFor(model => model.In_Personnummer)
        </dd>
    </dl>
</div>
<form asp-action="DeleteInstruktor">
    <input type="submit" value="Radera instruktör" class="btn btn-danger" />
</form>
<br/>
<div>
    <a asp-action="ReadAllaInstruktorer" class="btn btn-dark">Tillbaka</a>
</div>