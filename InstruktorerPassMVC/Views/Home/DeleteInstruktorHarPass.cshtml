@model InstruktorerPassMVC.Models.InstruktorerPass

@{
    ViewData["Title"] = "Delete Instruktor Har Pass";
}

<h2>Är du säker på att du vill radera vald aktivitet från instruktören?</h2>
<br />
<h4>Aktivitet: @Html.DisplayFor(model => model.IP_Id)</h4>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Id
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IP_Id)
        </dd>
        <dt>
            Instruktörens Id
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IP_InstruktorId)
        </dd>
        <dt>
            Aktivitetens Id
        </dt>
        <dd>
            @Html.DisplayFor(model => model.IP_PassId)
        </dd>
    </dl>
</div>
<form asp-action="DeleteInstruktorHarPass">
    <input type="submit" value="Radera aktivitet" class="btn btn-danger" />
</form>
<br />
<div>
    <a asp-action="ReadAllaInstruktorerHarPass" class="btn btn-dark">Tillbaka</a>
</div>